{
  "editor.lineHeight": 16,
  "editor.fontWeight": 600,
  "editor.fontSize": 15,
  "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.cursorBlinking": "smooth",
  "workbench.colorCustomizations": {
    "statusBar.background": "#267e68",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.noFolderBackground": "#333"
  },
  "explorer.sortOrder": "type",
  "files.trimFinalNewlines": true,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    "/.classpath": true,
    "/.project": true,
    "/.settings": true,
    "/.factorypath": true
  },
  "html.customData": ["v-model"],
  "liveServer.settings.donotShowInfoMsg": true,
  "search.quickOpen.includeSymbols": true,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "Pink Candy Dark",
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "vscode-icons",
  "npm.keybindingsChangedWarningShown": true,
  "explorer.confirmDelete": true,
  "diffEditor.maxFileSize": 100,
  "dotnet.help.firstView": "gettingStarted",
  "extensions.confirmedUriHandlerExtensionIds": [
    "ms-dotnettools.dotnet-interactive-vscode",
    "ms-dotnettools.vscode-dotnet-pack"
  ],
  "workbench.editorAssociations": {
    "*.class": "default"
  },
  "editor.formatOnSave": true,
  "javascript.format.enable": true,
  "javascript.format.insertSpaceAfterCommaDelimiter": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "vscode-pets.throwBallWithMouse": true,
  "vscode-pets.petType": "dog",
  "vscode-pets.petSize": "medium",
  "vscode-pets.petColor": "white",
  "vscode-pets.theme": "castle",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoSurround": "never",
  "intelephense.files.associations": ["*.php", "*.phtml"],
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "prettier.bracketSameLine": true,
  "prettier.enableDebugLogs": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "files.autoSave": "afterDelay",
  "bladeFormatter.format.enabled": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "cmake.configureOnOpen": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "workbench.editor.enablePreview": false,
  "git.openRepositoryInParentFolders": "never",
  "security.workspace.trust.untrustedFiles": "open"
}
